<?xml version="1.0" encoding="utf-8"?>
<s:TitleWindow xmlns:fx="http://ns.adobe.com/mxml/2009" 
			   xmlns:s="library://ns.adobe.com/flex/spark" 
			   xmlns:mx="library://ns.adobe.com/flex/mx" width="600" height="600"
			   title="商品录入">
	<!-- 添加商品界面 -->
	<fx:Declarations>
		<!-- 将非可视元素（例如服务、值对象）放在此处 -->
	</fx:Declarations>
	<s:Label text="条形码:" x="10" y="10"/><s:TextInput id="item_id" x="50" y="10"/>
	<s:Label text="名称:" x="200" y="10"/><s:TextInput id="name1" x="240" y="10"/>
	<s:Label text="类型:" x="10" y="40"/><s:TextInput id="item_type" x="50" y="40"/>
	<s:Label text="进货价:" x="198" y="40"/><s:TextInput id="inprice" x="240" y="40"/>
	<s:Label text="统一出货价" x="10" y="70"/><s:TextInput id="outprice" x="56" y="70"/>
	<s:Button id="sumitBtn" label="提交" x="130" y="230"></s:Button>
	<s:Button id="cancelBtn" label="重置" x="220" y="230"></s:Button>
</s:TitleWindow>
